<Window x:Class="didactic_palm_tree.NewDiagram"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:didactic_palm_tree"
        mc:Ignorable="d"
        Title="New Diagram" Height="131.578" Width="327.632">
    <Grid Margin="0,0,0,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="45"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0" x:Name="NameBox" Margin="10,10,10,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Height="25" />
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Button Grid.Column="0" x:Name="OkButton" Click="OkButton_OnClick" Content="OK" HorizontalAlignment="Right" Margin="20,10,20,10" VerticalAlignment="Top" Width="75"/>
            <Button Grid.Column="1" x:Name="CancelButton" Click="CancelButton_OnClick" Content="Cancel" HorizontalAlignment="Left" Margin="20,10,20,10" VerticalAlignment="Top" Width="75"/>
        </Grid>
    </Grid>
</Window>
